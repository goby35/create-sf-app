{
  "name": "lens-builder-env",
  "version": "1.0.0",
  "description": "Lens Protocol V2 Builder Environment - TypeScript project for building on Lens Chain Testnet",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "example:create-profile": "ts-node src/examples/create-profile.ts",
    "example:query-profiles": "ts-node src/examples/query-profiles.ts"
  },
  "keywords": [
    "lens",
    "lens-protocol",
    "blockchain",
    "web3",
    "typescript",
    "testnet"
  ],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "@lens-chain/sdk": "^1.0.3",
    "@lens-chain/storage-client": "^1.0.6",
    "@lens-protocol/client": "^0.0.0-canary-20250430134539",
    "@lens-protocol/metadata": "^2.1.0",
    "dotenv": "^17.2.3",
    "nft.storage": "^7.2.0",
    "pinata-web3": "^0.5.4",
    "viem": "^2.39.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
